<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/qf"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/purpose"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/breaker_name"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/breaker_type"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/breaker_it"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/breaker_instance"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/test_current"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/time_trip_1"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/time_trip_2"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/time_trip_3"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/uzo_name"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/uzo_nominal_current"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/uzo_current_type"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/uzo_diff_current"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/kabel"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/number_of_conductors"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/phase"
        android:textColor="@android:color/holo_blue_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/isolation"
        android:textColor="@android:color/holo_red_dark"/>
    <EditText
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:id="@+id/loop"
        android:textColor="@android:color/holo_blue_dark"/>
</LinearLayout>